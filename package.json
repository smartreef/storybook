{
	"name": "smartreef-storybook",
	"version": "0.0.1",
	"description": "StorybookJS boilerplate with TailwindCSS",
	"main": "index.js",
	"scripts": {
		"start": "start-storybook -p 6006",
		"build:storybook": "build-storybook -o dist",
		"build:tailwind": "postcss src/css/base.css -o styles/tailwind.css",
		"build:tailwindWatch": "postcss src/css/base.css -o styles/tailwind.css --watch",
		"pretest": "npm run build:tailwind",
		"test:updateSnapshot": "jest --updateSnapshot",
		"test": "jest --json --outputFile=tests/.jest-test-results.json",
		"test:dev": "jest"
	},
	"keywords": [
		"smartreef",
		"storybookjs",
		"postcss",
		"tailwindcss"
	],
	"author": "Smart Reef Pty Limited",
	"license": "ISC",
	"dependencies": {
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	},
	"devDependencies": {
		"@babel/core": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"@storybook/addon-actions": "^6.0.0-beta.38",
		"@storybook/addon-controls": "^6.0.0-beta.38",
		"@storybook/addon-docs": "^6.0.0-beta.38",
		"@storybook/addon-jest": "^6.0.0-beta.38",
		"@storybook/addon-links": "^6.0.0-beta.38",
		"@storybook/addon-notes": "^5.3.19",
		"@storybook/addon-storyshots": "^6.0.0-beta.38",
		"@storybook/addon-storysource": "^6.0.0-beta.38",
		"@storybook/addon-viewport": "^6.0.0-beta.38",
		"@storybook/addons": "^6.0.0-beta.38",
		"@storybook/preset-create-react-app": "^3.1.2",
		"@storybook/react": "^6.0.0-beta.38",
		"@storybook/theming": "^6.0.0-beta.38",
		"autoprefixer": "^9.8.4",
		"babel-jest": "^26.1.0",
		"babel-loader": "^8.1.0",
		"babel-plugin-react-require": "^3.1.3",
		"eslint": "^7.4.0",
		"eslint-config-google": "^0.14.0",
		"eslint-plugin-jest": "^23.17.1",
		"eslint-plugin-react": "^7.20.3",
		"eslint-plugin-sort-keys-fix": "^1.1.1",
		"jest": "^26.1.0",
		"jest-css-modules": "^2.1.0",
		"markdown-loader-jest": "^0.1.1",
		"postcss-cli": "^7.1.1",
		"postcss-flexbugs-fixes": "^4.2.1",
		"postcss-import": "^12.0.1",
		"postcss-nesting": "^7.0.1",
		"postcss-preset-env": "^6.7.0",
		"react-test-renderer": "^16.13.1",
		"tailwindcss": "^1.4.6"
	}
}
